{
    "presets": [
        "mobx", [
            "env",
            {
                "targets": {
                    "browsers": "> 2%"
                }
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }],
        ["@babel/plugin-syntax-decorators"],
        ["import", {"libraryName": "antd", "style": "css"}]
    ]
}